{
  "name": "@feryardiant/admin-web",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "types": "types.d.ts",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite-ssg build",
    "build:dev": "vite-ssg build --mode development",
    "lint": "eslint . --ext .js,.vue",
    "lint:fix": "eslint . --ext .js,.vue --fix",
    "serve": "vite preview",
    "test": "web-test-runner --config tests/web-test-runner.config.mjs"
  },
  "dependencies": {
    "js-cookie": "^3.0.1",
    "nprogress": "^0.2.0",
    "qs": "^6.11.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.25.2",
    "@types/markdown-it": "^12.2.3",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/server-renderer": "^3.2.37",
    "@vue/test-utils": "^2.0.2",
    "@vueuse/core": "^7.7.1",
    "@vueuse/head": "^0.7.9",
    "@web/test-runner": "^0.13.31",
    "chai": "^4.3.6",
    "dotenv": "^10.0.0",
    "eslint": "^8.21.0",
    "firebase": "^9.9.1",
    "gray-matter": "^4.0.3",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.6.4",
    "markdown-it-link-attributes": "^4.0.0",
    "postcss": "^8.4.14",
    "postcss-nested": "^5.0.6",
    "rollup": "^2.77.2",
    "typescript": "^4.7.4",
    "unplugin-vue-components": "^0.17.21",
    "vite": "^2.9.14",
    "vite-plugin-md": "^0.11.9",
    "vite-plugin-pages": "^0.21.4",
    "vite-plugin-pwa": "^0.11.13",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vite-plugin-windicss": "^1.8.7",
    "vite-ssg": "^0.17.11",
    "vite-web-test-runner-plugin": "^0.1.0",
    "windicss": "^3.5.6",
    "workbox-window": "^6.5.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "eslintConfig": {
    "extends": [
      "@antfu/vue"
    ],
    "globals": {
      "vi": true,
      "vitest": true
    },
    "ignorePatterns": [
      "**/*.d.ts"
    ],
    "rules": {
      "vue/component-definition-name-casing": [
        "error",
        "kebab-case"
      ],
      "vue/html-self-closing": "off"
    },
    "overrides": [
      {
        "files": [
          "*.spec.js"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  }
}
